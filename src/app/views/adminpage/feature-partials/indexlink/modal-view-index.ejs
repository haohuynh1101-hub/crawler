<!-- The Modal -->
<div class="modal" id="view-modal-indexlink">
        <div class="modal-dialog">
            <div class="modal-content">
    
    
                <div class="modal-header">
                    <h4 class="modal-title">Index link</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
    
                
    
                    <div class="modal-body">
    
                        <div class="form-group">
                            <label for="links">Tên dự án</label>
                            <input id='view-name-indexlink' disabled type="text" name='name' class="form-control" required>
                        </div>
    
                        <div class="form-group">
                            <label for="links">Danh sách link ( mỗi link trên 1 dòng)</label>
                            <textarea id="view-links-indexlink" disabled id="links" name='links' class="form-control" required></textarea>
                        </div>
    
                    </div>
    
                    <div class="modal-footer">
                        <button type="button" class="btn btn-danger" data-dismiss="modal">Đóng</button>
                    </div>
    
               
            </div>
        </div>
    </div>