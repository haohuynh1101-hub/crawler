<!--  Modal content for the above example -->
<div class="modal fade bs-example-modal-lg" tabindex="-1" role="dialog" id="backlink-modal"
    aria-labelledby="backlink-form" aria-hidden="true" style="display: none;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content pb-0">
            <div class="modal-header">
                <h4 class="modal-title" id="backlink-form">Backlink Form</h4>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            </div>
            <div class="modal-body pb-0">
                <div class="row">
                    <div class="col-md-6">

                        <div class="form-group row">
                            <label class="col-3 col-form-label pr-0">Tên dự án</label>
                            <div class="col-8">
                                <input id='projectName_backlink' type="text" class="form-control pl-0" data-focus=" Website"
                                    value="">
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-3 col-form-label pr-0">URL Backlink</label>
                            <div class="col-8">
                                <input id='url_backlink' type="text" class="form-control pl-0" data-focus=" Website"
                                    value="">
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-3 col-form-label pr-0">URl chính cần view</label>
                            <div class="col-8">
                                <input id='main_url' type="text" class="form-control pl-0" data-focus=" Backlink"
                                    value="">
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-3 col-form-label pr-0">Từ khóa (cách nhau bởi dấu ",")</label>
                            <div class="col-8">
                                <input id='keyword_backlink' type="text" class="form-control pl-0"
                                    data-focus=" Backlink" value="">
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-3 col-form-label">Delay (giây)</label>
                            <div class="col-8">
                                <input type="text" id='delay_backink' onkeypress="acceptPositiveNumber(event)"
                                    class="form-control" data-focus=" Delay" value="">
                            </div>
                        </div>

                        <div class="form-group row">
                            <label class="col-3 col-form-label">Số lượng traffic</label>
                            <div class="col-8">
                                <select id='amount_backlink' class="form-control">
                                    <option value="500">500</option>
                                    <option value="1000">1000</option>
                                    <option value="5000">5000</option>
                                    <option value="10000">10000</option>
                                </select>
                            </div>
                        </div>

                    </div>
                    <div class="col-md-6">
                        <div class="row form-group">
                            <div class="checkbox">
                                <input id="checkbox4" type="checkbox" checked disabled>
                                <label for="checkbox4">
                                    Tự động xóa cookies
                                </label>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="checkbox">
                                <input id="checkbox3" type="checkbox" checked disabled>
                                <label for="checkbox3">
                                    Tự động thay đổi UserAgent
                                </label>
                            </div>
                        </div>
                        <div class="form-group row" style="visibility: hidden">
                            <div class="checkbox">
                                <input id="" type="checkbox">
                                <label for="">
                                    .
                                </label>
                            </div>
                        </div>
                        <div class="form-group row" style="visibility: hidden">
                            <div class="checkbox">
                                <input id="" type="checkbox">
                                <label for="">
                                    .
                                </label>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-9"></div>
                            <div class="col-3">
                                <div class="row">
                                    <button id='btnStartBacklink' onclick='saveBackLinkProject()' type="button"
                                        class="btn btn-outline-success w-100 btn-lg">Lưu</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->